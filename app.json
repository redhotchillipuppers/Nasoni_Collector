{
  "expo": {
    "name": "nasoni-collector",
    "slug": "nasoni-collector",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "nasonicollector",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spaceyemu.nasonicollector",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow Nasoni Collector to use your location to help you find and collect nasoni.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Allow Nasoni Collector to use your location to help you find and collect nasoni.",
        "NSLocationAlwaysUsageDescription": "Allow Nasoni Collector to use your location to help you find and collect nasoni."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.spaceyemu.nasonicollector"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Nasoni Collector to use your location to help you find and collect nasoni."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "4eaa48a6-2f8b-4f0b-b7fe-6a98986f6b0d"
      }
    }
  }
}
